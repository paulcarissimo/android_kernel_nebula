ui_print("    ***                  ***  ");
ui_print("      ***              ***    ");
ui_print("        ***          ***     ");
ui_print("          ***      ***       ");
ui_print("            ***  ***       ");
ui_print("              ****          ");
ui_print("               **          ");
ui_print("              ****          ");
ui_print("            ***  ***      ");
ui_print("          ***      ***    ");
ui_print("        ***          ***    ");
ui_print("      ***              ***    ");
ui_print("    ***                  ***   ");
ui_print(" ");
